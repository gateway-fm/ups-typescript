{
    "name": "@gateway-fm/react",
    "version": "0.1.6",
    "description": "React hooks and providers for UPS x402 SDK",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gateway-fm/ups-typescript.git"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "zustand": "^4.5.0",
        "@gateway-fm/sdk": "workspace:*"
    },
    "peerDependencies": {
        "react": "^19.2.4",
        "@tanstack/react-query": "^5.0.0",
        "@gateway-fm/sdk": "workspace:*"
    },
    "devDependencies": {
        "react": "^19.2.4",
        "@types/react": "^19.2.10",
        "tsup": "^8.0.0",
        "typescript": "^5.5.0"
    }
}
